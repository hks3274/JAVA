package t01;

import java.util.Scanner;

public class Test4 {
	public static void main(String[] args) { //실행메소드는 하나의 프로젝트에서는 하나만 있어야 한다. void -> 리턴값이 없을때 사용
		//변수를 () 안에 지정
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		
		System.out.println("두 입력의 합은"+sum(a,b)+"입니다.");
	}
	
	static int sum(int a, int b) {
		return a + b;
	}
}
